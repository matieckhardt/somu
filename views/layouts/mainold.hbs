<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Website switchit">
    <meta name="author" content="SwitchIT.com.ar Argentina">
    <meta name="robots" content="noarchive">

    <title>Soluciones Modulares - Home</title>

    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <!-- CSS -->
    <link href="/css/main.css" rel="stylesheet">
    <!-- Slick -->
    <link rel="stylesheet" type="text/css"
        href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.css" />
    <link rel="stylesheet" type="text/css"
        href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick-theme.min.css" />
    <!-- FontAwesome -->
    <link rel="stylesheet" href="https://use.typekit.net/mwr2veg.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/css/all.min.css">
    <!-- Animate Style -->
    <link rel="stylesheet" href="/css/animate-small.css" />
    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="180x180" href="/img/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/img/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/img/favicon/favicon-16x16.png">
    <link rel="manifest" href="/img/favicon/manifest.json">
    <link rel="mask-icon" href="/img/favicon/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
</head>


<body>
    <div class="container-fluid">
        <div class="row wrapper min-vh-100 flex-column flex-sm-row">

            {{>sidebar}}
        <main class="col bg-white"></main>
            {{{body}}}
            {{> footer }}
 </main>
        </div>
    </div>

    <!-- Modal Upload Image -->
    <div id="uploadImg" class="modal fade" role="dialog">
        <div class="modal-dialog modal-dialog-centered modal-lg">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header bg-primary text-success">
                    <h5 class="modal-title">Subir imagen al carrusel</h5>
                    <button type="button" class="close text-success" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true"><i class="fa fa-times-circle fa-lg"></i></span>
                    </button>
                </div>
                <div class="modal-body">
                    <!-- Form upload -->
                    <form id="formulario-archivo-nuevo">
                        <div class="custom-file mb-1">
                            <input type="file" class="custom-file-input" id="customFile" accept="image/jpeg" multiple />
                            <label class="custom-file-label" for="customFile">Selecciona la imagen para subir al
                                carrusel</label>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button onclick="guardarImagenDeCarrousel()" type="button" class="btn btn-success">GUARDAR
                        CAMBIOS</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal Sort Images -->
    <div id="sortImg" class="modal fade" role="dialog">
        <div class="modal-dialog modal-dialog-centered modal-xl">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header bg-primary text-success">
                    <h4 class="modal-title">Ordenar Imágenes del Carrusel</h4>
                    <button type="button" class="close text-success" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true"><i class="fa fa-times-circle fa-lg"></i></span>
                    </button>
                </div>
                <div class="modal-body">
                    <!-- Preview-->
                    <label for="preview">Arrastre las imagenes para ordenarlas</label>
                    <div class="row p-0" id='preview'></div>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal Delete Image -->
    <div id="deleteImg" class="modal fade" role="dialog">
        <div class="modal-dialog modal-dialog-centered modal-xl">
            <!-- Modal content -->
            <div class="modal-content">
                <div class="modal-header bg-primary text-success">
                    <h4 class="modal-title">Eliminar Imágenes del Carrusel</h4>
                    <button type="button" class="close text-success" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true"><i class="fa fa-times-circle fa-lg"></i></span>
                    </button>
                </div>
                <div class="modal-body">
                    <!-- Delete Carousel -->
                    <label for="deleteCar">Haga click en la que desea eliminar</label>
                    <div class="row p-0" id='deleteCar'></div>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal Confirm Delete? -->
    <div class="modal fade" id="confirmDelete" tabindex="-1" role="dialog" aria-labelledby="deleteProduct"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header bg-primary text-success">
                    <h5 class="modal-title">Eliminar Imagen?</h5>
                    <button type="button" class="close text-success" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true"><i class="fa fa-times-circle fa-lg"></i></span>
                    </button>
                </div>
                <div class="modal-body">
                    Se eliminara la imagen seleccionada.
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger">ELIMINAR</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Logo Whatsapp Mobile -->
    <div class="d-none" id="wzp">
        <a target="_blank"
            href="https://wa.me/5491158888766?text=Estoy%20interesado%20en%20recibir%20atencion%20personalizada"><i
                class="fab fa-whatsapp wzp-float" aria-hidden="true"></i></a>
    </div>

    <!-- jQuery, Popper.js, and  Bootstrap JS-->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"
        integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    </script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
        integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous">
    </script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"
        integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous">
    </script>
    <!-- Slick JS -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.js">
    </script>
    <!-- Scroll Reveal WOW -->
    <script src="/scripts/wow.min.js"></script>
    <!-- Script associated -->
    <script src="/index.js"></script>
</body>

</html>